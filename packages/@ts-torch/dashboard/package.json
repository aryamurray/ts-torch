{
  "name": "@ts-torch/dashboard",
  "version": "0.1.0",
  "description": "Real-time TUI training dashboard for ts-torch",
  "homepage": "https://github.com/aryamurray/ts-torch#readme",
  "bugs": {
    "url": "https://github.com/aryamurray/ts-torch/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aryamurray/ts-torch.git",
    "directory": "packages/@ts-torch/dashboard"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vite build",
    "dev": "tsc --watch",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "typescript": "5.9.2",
    "vitest": "^4.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
