{
  "name": "@ts-torch/core",
  "version": "0.1.3",
  "description": "Core tensor operations & FFI bindings for ts-torch",
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aryamurray/ts-torch.git",
    "directory": "packages/@ts-torch/core"
  },
  "bugs": {
    "url": "https://github.com/aryamurray/ts-torch/issues"
  },
  "homepage": "https://github.com/aryamurray/ts-torch#readme",
  "scripts": {
    "build": "vite build",
    "build:native": "cd native && bun x cmake-js build --release",
    "build:native:cuda": "bun run ../../../scripts/build-native-cuda.ts",
    "dev": "tsc --watch",
    "check-types": "tsc --noEmit",
    "test": "vitest run --dangerouslyIgnoreUnhandledErrors",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "install": "cd native && bun x cmake-js rebuild"
  },
  "dependencies": {
    "@ts-torch-platform/loader": "^0.1.0"
  },
  "optionalDependencies": {
    "node-addon-api": "^8.0.0",
    "cmake-js": "^7.3.0"
  },
  "devDependencies": {
    "typescript": "5.9.2",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=18"
  }
}
