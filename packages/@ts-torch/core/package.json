{
  "name": "@ts-torch/core",
  "version": "0.1.0",
  "description": "Core tensor operations & FFI bindings for ts-torch",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "build:native": "bun run scripts/build-native.ts",
    "dev": "tsc --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@ts-torch-platform/loader": "workspace:*",
    "koffi": "^2.9.0"
  },
  "devDependencies": {
    "typescript": "5.9.2",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=18"
  }
}
