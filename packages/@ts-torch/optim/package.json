{
  "name": "@ts-torch/optim",
  "version": "0.1.0",
  "description": "Optimizers for ts-torch",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@ts-torch/core": "workspace:*"
  },
  "devDependencies": {
    "typescript": "5.9.2",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=18"
  }
}
