{
  "name": "@ts-torch/torch",
  "version": "0.1.4",
  "description": "PyTorch bindings for TypeScript/JavaScript",
  "keywords": [
    "pytorch",
    "torch",
    "tensor",
    "machine-learning",
    "deep-learning",
    "neural-network",
    "typescript"
  ],
  "license": "MIT",
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aryamurray/ts-torch.git",
    "directory": "packages/@ts-torch/torch"
  },
  "bugs": {
    "url": "https://github.com/aryamurray/ts-torch/issues"
  },
  "homepage": "https://github.com/aryamurray/ts-torch#readme",
  "scripts": {
    "build": "vite build",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@ts-torch/core": "^0.1.4",
    "@ts-torch/nn": "^0.1.4",
    "@ts-torch/optim": "^0.1.4",
    "@ts-torch/datasets": "^0.1.4",
    "@ts-torch/train": "^0.1.4",
    "@ts-torch/rl": "^0.1.4"
  },
  "devDependencies": {
    "typescript": "5.9.2",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.5.4"
  },
  "engines": {
    "node": ">=18"
  }
}
