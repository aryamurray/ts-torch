{
  "name": "@ts-torch-platform/loader",
  "version": "0.1.0",
  "description": "Platform-specific binary loader for ts-torch",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint src"
  },
  "optionalDependencies": {
    "@ts-torch-platform/win32-x64": "workspace:*",
    "@ts-torch-platform/darwin-x64": "workspace:*",
    "@ts-torch-platform/darwin-arm64": "workspace:*",
    "@ts-torch-platform/linux-x64": "workspace:*",
    "@ts-torch-platform/linux-arm64": "workspace:*"
  },
  "devDependencies": {
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
